digraph {
	a0 [label=a]
	b1 [label=b]
	b2 [label=b]
	c3 [label=c]
	c4 [label=c]
	c5 [label=c]
	c6 [label=c]
	0 [label=0]
	1 [label=1]
	a0 -> b1 [color=green]
	a0 -> b2 [color=red]
	b1 -> c3 [color=green]
	b1 -> c4 [color=red]
	b2 -> c5 [color=green]
	b2 -> c6 [color=red]
	c3 -> 1 [color=green]
	c3 -> 1 [color=red]
	c4 -> 1 [color=green]
	c4 -> 1 [color=red]
	c5 -> 0 [color=green]
	c5 -> 0 [color=red]
	c6 -> 0 [color=green]
	c6 -> 0 [color=red]
}
